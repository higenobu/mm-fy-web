DirectoryIndex index.php

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    
    # Don't rewrite if file exists
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    
    # Route everything through index.php
    RewriteRule ^(.*)$ index.php [L,QSA]
</IfModule>

# Security
<FilesMatch "^\.">
    Require all denied
</FilesMatch>

